<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="ir_cron_oidc_cleanup_tokens" model="ir.cron">
        <field name="name">OIDC Cleanup Tokens</field>
        <field name="model_id" ref="model_auth_oidc_token"/>
        <field name="state">code</field>
        <field name="code">model.cron_cleanup_expired()</field>
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="active" eval="True"/>
    </record>
    <record id="ir_cron_oidc_cleanup_codes" model="ir.cron">
        <field name="name">OIDC Cleanup Authorization Codes</field>
        <field name="model_id" ref="model_auth_oidc_authorization_code"/>
        <field name="state">code</field>
        <field name="code">model.cron_cleanup_expired()</field>
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
